        "method": "POST",
        "url": "http://localhost:3000/api/users/login",
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "username": "testuser001",
          "password": "WrongPassword123!"
        },
        "description": "Invalid password (should return 401 error)"
      }
