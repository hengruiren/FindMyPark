{
  "baseUrl": "http://localhost:3000/api/users",
  "testCases": {
    "1_REGISTER": {
      "1.1_Success": {
        "method": "POST",
        "url": "http://localhost:3000/api/users/register",
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "username": "testuser001",
          "email": "testuser001@example.com",
          "password": "SecurePassword123!"
        },
        "description": "Register a new user"
      },
      "1.2_MissingFields": {
        "method": "POST",
        "url": "http://localhost:3000/api/users/register",
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "username": "testuser",
          "password": "password123"
        },
        "description": "Missing required fields (should return 400 error)"
      },
      "1.3_DuplicateUsername": {
        "method": "POST",
        "url": "http://localhost:3000/api/users/register",
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "username": "testuser001",
          "email": "different@example.com",
          "password": "password123"
        },
        "description": "Duplicate username (should return 400 error)"
      },
      "1.4_DuplicateEmail": {
        "method": "POST",
        "url": "http://localhost:3000/api/users/register",
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "username": "differentuser",
          "email": "testuser001@example.com",
          "password": "password123"
        },
        "description": "Duplicate email (should return 400 error)"
      }
    },
